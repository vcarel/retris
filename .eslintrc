{
  "extends": [
    "react-app",
    "standard",
    "standard-jsx"
  ],
  "rules": {
    "no-console": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "standard/computed-property-even-spacing": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": "warn",
    "linebreak-style": "error"
  }
}
